<div class="container py-5">
    <div class="row text-center text-white mb-5">
        <div class="col-lg-7 mx-auto">
            <h2 class="display-4">Listado</h2>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="main-box clearfix">
                    <div class="table-responsive">
                        <table class="table user-list">
                            <thead>
                                <tr>
                                    <th class="text-center"><span>Status</span></th>
                                    <th><span>Usuario</span></th>
                                    <th><span>Rut</span></th>
                                    <th><span>Email</span></th>
                                    <th><span>Tipo Permiso</span></th>
                                    <th>&nbsp;</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each usuario}}
                                <tr>
                                    <!--ID-->
                                    <td class="text-center">
                                        <span class="label label-danger">[ID:{{this.idUser}}]</span>
                                    </td>
                                    <!--NOMBRE-->
                                    <td>
                                        <img src="https://www.seekpng.com/png/detail/73-730482_existing-user-default-avatar.png" alt="">
                                        <a class="user-link">{{this.nombre}}  {{this.apellido}}</a>
                                        <span class="user-subhead">Registrado</span>
                                    </td>
                                    <!--RUT-->
                                    <td>
                                        {{this.rut}}
                                    </td>
                                    <!--CORREO-->
                                    <td>
                                        <a href="#">{{this.email}}</a>
                                    </td>
                                    <!--TIPO PERMISO-->
                                    <td>
                                        {{this.tipoPermiso}}
                                    </td>
                                    <!--DEMASES-->
                                    <td style="width: 20%;">
                                        <a href="#" class="table-link">
                                            <span class="fa-stack">
                                                <i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>
                                            </span>
                                        </a>
                                        <a href="#" class="table-link">
                                            <span class="fa-stack">
                                                <i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-pencil fa-stack-1x fa-inverse"></i>
                                            </span>
                                        </a>
                                        <a href="#" class="table-link danger">
                                            <span class="fa-stack">
                                                <i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-trash-o fa-stack-1x fa-inverse"></i>
                                            </span>
                                        </a>
                                    </td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>